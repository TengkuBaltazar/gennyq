        <bpmn2:script>userToken = (GennyToken)userToken;
BaseEntityUtils beUtils = new BaseEntityUtils(userToken, serviceToken);
newUser = beUtils.create(((GennyToken)userToken).getUserCode(), ((GennyToken)userToken).getUserCode());
/* copy from userToken */
List&lt;Answer> answers = new CopyOnWriteArrayList&lt;Answer>();
Answer firstNameAnswer = new Answer(beUtils.getGennyToken().getUserCode(),newUser.getCode(), &quot;PRI_FIRSTNAME&quot;, userToken.getString(&quot;given_name&quot;));
Answer lastNameAnswer = new Answer(beUtils.getGennyToken().getUserCode(),newUser.getCode(), &quot;PRI_LASTNAME&quot;, userToken.getString(&quot;family_name&quot;));
Answer emailAnswer = new Answer(beUtils.getGennyToken().getUserCode(),newUser.getCode(), &quot;PRI_EMAIL&quot;, userToken.getString(&quot;email&quot;));
newUser = beUtils.updateBaseEntity(newUser, firstNameAnswer,BaseEntity.class);
newUser = beUtils.updateBaseEntity(newUser, lastNameAnswer,BaseEntity.class);
newUser = beUtils.updateBaseEntity(newUser, emailAnswer,BaseEntity.class);
answers.add(firstNameAnswer);
answers.add(lastNameAnswer);
answers.add(emailAnswer);
beUtils.saveAnswers(answers,false);
kcontext.setVariable(&quot;newUser&quot;,newUser);
kcontext.setVariable(&quot;userCode&quot;, ((GennyToken)userToken).getUserCode());

System.out.println(&quot;userlifeCycle: New User Created: pid: &quot;+callingProcessId +&quot;:&quot; +newUser+&quot; now sendng to channel callback :[&quot;+((GennyToken)userToken).getUserCode()+&quot;]&quot; );
</bpmn2:script>
