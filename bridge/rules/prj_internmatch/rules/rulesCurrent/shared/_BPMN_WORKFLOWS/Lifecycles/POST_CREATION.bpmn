      <bpmn2:script>BaseEntityUtils beUtils = new BaseEntityUtils(userToken);

/* CHECK PRI_PROFILE FOR HC */

String hcCode = newBe.getValue(&quot;LNK_HOST_COMPANY&quot;, null);
System.out.println(&quot;hcCode = &quot; + hcCode);

/* By default, assume it is an old CPY that is complete */
hcProfileComplete = true;

if (hcCode == null) {
	System.out.println(&quot;hcCode is NULL&quot;);
} else {
	hcCode = hcCode.replace(&quot;\&quot;&quot;, &quot;&quot;).replace(&quot;[&quot;, &quot;&quot;).replace(&quot;]&quot;, &quot;&quot;);
	System.out.println(&quot;hcCode = &quot; + hcCode);

	BaseEntity linkedHC = beUtils.getBaseEntityByCode(hcCode);
	System.out.println(&quot;linkedHC = &quot; + linkedHC);
	if (linkedHC == null) {
		System.out.println(&quot;linkedHC is NULL&quot;);
	} else {

		String profile = linkedHC.getValue(&quot;PRI_PROFILE&quot;, null);
		System.out.println(&quot;profile = &quot; + profile);
		if (profile == null) {
			System.out.println(&quot;profile is NULL&quot;);
		} else {

			if (profile.equals(&quot;Incomplete&quot;)) {
				hcProfileComplete = false;
			}
		}
	}
}
System.out.println(&quot;hcProfileComplete: &quot; + hcProfileComplete);
kcontext.setVariable(&quot;hcProfileComplete&quot;, hcProfileComplete);</bpmn2:script>
