      <bpmn2:script>System.out.println(&quot;%%%%%%%%%%%%%%%%%%% START HELPER FUNCTION WORKFLOW %%%%%%%%%%%%%%%%%%%&quot;);

/* Gather Session Facts and Tokens */
	SessionFacts sessionFacts2 = (SessionFacts)sessionFacts;

	serviceToken = (GennyToken)sessionFacts2.getServiceToken();
	userToken = (GennyToken)sessionFacts2.getUserToken();

	kcontext.setVariable(&quot;serviceToken&quot;, serviceToken);
	kcontext.setVariable(&quot;userToken&quot;, userToken);

	System.out.println(&quot;HELPER_FUNCTION: userToken = &quot; +userToken.getCode());
	System.out.println(&quot;HELPER_FUNCTION: serviceToken = &quot; +serviceToken.getCode());

	OutputParam sauce = (OutputParam)sessionFacts.getMessage();
	source = sauce.getAskSourceCode();
	kcontext.setVariable(&quot;source&quot;, source);

	System.out.println(&quot;source: &quot; + source);
	
	BaseEntityUtils beUtils = new BaseEntityUtils(userToken);
	uuid = UUID.randomUUID();
	kcontext.setVariable(&quot;uuid&quot;, uuid);
	System.out.println(&quot;uuid: &quot; + uuid);


	String newCode = &quot;STK_&quot;+uuid;
	newCode = newCode.toUpperCase();
	BaseEntity defBE = beUtils.getDEFByCode(&quot;DEF_SUPPORT_TICKET&quot;);
	newTicket = beUtils.create(defBE, newCode, newCode);
	kcontext.setVariable(&quot;newTicket&quot;, newTicket);
	System.out.println(&quot;newTicket = &quot; + newTicket);

</bpmn2:script>
