        <bpmn2:script>BaseEntityUtils beUtils = new BaseEntityUtils(serviceToken);

/* Get the parent internship of the application */
BaseEntity internshipBe = beUtils.getParent(newApplication.getCode(), &quot;LNK_BEG&quot;);

/*  Construct answer with Source, Target, Attribute Code, Value */
System.out.println(&quot;Setting PRI_IS_FULL = true for internship: &quot; + internshipBe.getCode());
Answer answer = new Answer(userToken.getUserCode(), internshipBe.getCode(), &quot;PRI_IS_FULL&quot;, &quot;true&quot;);
beUtils.saveAnswer(answer);


/* Get all child applications of internship */
List&lt;BaseEntity> childApplications = beUtils.getLinkedBaseEntities(internshipBe.getCode(), &quot;LNK_BEG&quot;);

for (BaseEntity app : childApplications) {
	System.out.println(&quot;Child Application: &quot; + app.getCode());
	String status = beUtils.getBaseEntityValue(app.getCode(), &quot;PRI_STATUS&quot;).toString();

	if ( status.equals(&quot;APPLIED&quot;) || status.equals(&quot;SHORTLISTED&quot;) || status.equals(&quot;INTERVIEWED&quot;) ) {
		/* IGNORE THE CURRENT APPLICATION SINCE IT IS BEING MOVED TO OFFERED */
		if (!app.getCode().equals(newApplication.getCode())) {
			System.out.println(&quot;Disabling Application: &quot; + app.getCode());
			answer = new Answer(userToken.getUserCode(), app.getCode(), &quot;PRI_DISABLED&quot;, &quot;true&quot;);
			beUtils.saveAnswer(answer);
		}
	}
}
</bpmn2:script>
