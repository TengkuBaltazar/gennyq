message.getData().setValue(&quot;QUE_DASHBOARD_VIEW&quot;);
message.setToken(userToken.getToken());

askFacts = new SessionFacts(serviceToken, userToken, message);
kcontext.setVariable(&quot;askFacts&quot;, askFacts);
*/</bpmn2:script>
