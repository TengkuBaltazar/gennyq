      <bpmn2:script>BaseEntityUtils beUtils = new BaseEntityUtils(userToken);

BaseEntity hostCompany = beUtils.getBaseEntityFromLNKAttr(newBe, &quot;LNK_HOST_COMPANY&quot;);
System.out.println(&quot;hostCompany = &quot; + hostCompany);

if (hostCompany != null) {
	String validation = hostCompany.getValue(&quot;PRI_VALIDATION&quot;, null);
	if (validation != null) {
	System.out.println(&quot;validation = &quot; + validation);
		if (!validation.equals(&quot;Ready&quot;) &amp;&amp; !validation.equals(&quot;Validated&quot;)) {
			
			/* SCHEDULE REMIDERS TO THE USER */
			LocalDateTime now = LocalDateTime.now(ZoneOffset.UTC);

			ShleemyUtils.scheduleMessage(userToken, &quot;SCHEDULE_REMIND_HCR_COMPLETE_DOCS_48HR&quot;, newBe.getCode()+&quot;_HCR_COMPLETE_DOCS_48HR&quot;, now.plusHours(48), newBe.getCode());

			ShleemyUtils.scheduleMessage(userToken, &quot;SCHEDULE_REMIND_HCR_COMPLETE_DOCS_96HR&quot;, newBe.getCode()+&quot;_HCR_COMPLETE_DOCS_96HR&quot;, now.plusHours(96), newBe.getCode());
