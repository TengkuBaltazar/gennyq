      <bpmn2:outgoing>SequenceFlow_3</bpmn2:outgoing>
      <bpmn2:script>
BaseEntityUtils beUtils = new BaseEntityUtils(userToken);
newTicket = beUtils.getBaseEntityByCode(newTicket.getCode());
BaseEntity userBe = beUtils.getBaseEntityByCode(userToken.getUserCode());

String ticketName = userBe.getValue(&quot;PRI_NAME&quot;, null);

String userFirstName = userBe.getValue(&quot;PRI_FIRSTNAME&quot;, null);

String ticketDescription = newTicket.getValue(&quot;PRI_PROBLEM&quot;, null);

String ticketLogRocketSession = newTicket.getValue(&quot;PRI_LOG_ROCKET_SESSION&quot;, null);

String imageUUID = newTicket.getValue(&quot;PRI_UPLOAD&quot;, null);

String ticketEmail = userBe.getValue(&quot;PRI_EMAIL&quot;, null);

BaseEntity preferredContactBe = beUtils.getBaseEntityFromLNKAttr(newTicket, &quot;LNK_PREFERRED_CONTACT&quot;);
String preferredContact = preferredContactBe.getValue(&quot;PRI_NAME&quot;, null);

String ticketMobile = userBe.getValue(&quot;PRI_MOBILE&quot;, null);

System.out.println(&quot;%%%%%%%%%%%%%%%%%%% SET UP NOTIFICATION DATA %%%%%%%%%%%%%%%%%%%&quot;);
System.out.println(&quot;ticketName : &quot; + ticketName);
System.out.println(&quot;ticketDescription : &quot; +  ticketDescription);
System.out.println(&quot;ticketLogRocketSession : &quot; +  ticketLogRocketSession);
System.out.println(&quot;ticketEmail : &quot; + ticketEmail);
System.out.println(&quot;ticketMobile : &quot; + ticketMobile);
System.out.println(&quot;preferredContact : &quot; + preferredContact);

String imageUrl = null;

if(imageUUID != null)
{
	imageUrl = GennySettings.projectUrl + &quot;/web/public/&quot; + imageUUID;
}

System.out.println(&quot;ticketImage : &quot; +  imageUrl);

String ticketPreferredContact = null;


if (preferredContact.contains(&quot;Email&quot;))
{
	ticketPreferredContact = &quot;Email&quot;;
	
	recipient = ticketEmail;
	notificationTypeForUser = QBaseMSGMessageType.EMAIL;

	templateDataForUser = new HashMap&lt;String, String>();
	templateDataForUser.put(&quot;name&quot;, userFirstName);
	templateDataForUser.put(&quot;description&quot;, ticketDescription);
	templateDataForUser.put(&quot;image&quot;, imageUrl);
}

if (preferredContact.contains(&quot;SMS&quot;))
{
	ticketPreferredContact = &quot;SMS&quot;;
	recipient = ticketMobile;
	notificationTypeForUser = QBaseMSGMessageType.SMS;
	templateDataForUser = new HashMap&lt;String, String>();
	String smsBody = &quot;Hi &quot; + userFirstName  + &quot;, Thank you for generating a Support Ticket for Internmatch. One of our agents will get back to you with a solution shortly. &quot;;
	templateDataForUser.put(&quot;smsBody&quot;, smsBody);
}

System.out.println(&quot;ticketPreferredContact : &quot; +  ticketPreferredContact);

kcontext.setVariable(&quot;notificationTypeForUser&quot;, notificationTypeForUser);
kcontext.setVariable(&quot;templateDataForUser&quot;, templateDataForUser);
kcontext.setVariable(&quot;recipient&quot;, recipient);



beUtils.saveAnswer(new Answer(userToken.getUserCode(), newTicket.getCode(), &quot;PRI_USER_CODE&quot;, userToken.getUserCode()));
beUtils.saveAnswer(new Answer(userToken.getUserCode(), newTicket.getCode(), &quot;PRI_NAME&quot;, ticketName));



templateData = new HashMap&lt;String, String>();
templateData.put(&quot;name&quot;, ticketName);
templateData.put(&quot;mobile&quot;, ticketMobile);
templateData.put(&quot;email&quot;, ticketEmail);
templateData.put(&quot;description&quot;, ticketDescription);
templateData.put(&quot;url&quot;, ticketLogRocketSession);
templateData.put(&quot;preferredContact&quot;, ticketPreferredContact);
templateData.put(&quot;image&quot;, imageUrl);

notificationType = QBaseMSGMessageType.EMAIL;
kcontext.setVariable(&quot;notificationType&quot;, notificationType);
kcontext.setVariable(&quot;templateData&quot;, templateData);

</bpmn2:script>
    </bpmn2:scriptTask>
