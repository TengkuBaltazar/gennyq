        <bpmn2:script>callingWorkflow = &quot;userLifecycle&quot;;
kcontext.setVariable(&quot;callingWorkflow&quot;, callingWorkflow);

serviceToken = (GennyToken)sessionFacts.getServiceToken();
userToken = (GennyToken)sessionFacts.getUserToken();

callingProcessToken = (CallingProcessToken) sessionFacts.getMessage();

kcontext.setVariable(&quot;callingProcessToken&quot;, callingProcessToken);
kcontext.setVariable(&quot;userToken&quot;, userToken);
kcontext.setVariable(&quot;serviceToken&quot;, serviceToken);

baseEntityType = &quot;StudentSelfSignUp&quot;;
kcontext.setVariable(&quot;baseEntityType&quot;, baseEntityType);

CallingProcessToken cpt = (CallingProcessToken)callingProcessToken;
callingProcessId = cpt.getProcessId();
kcontext.setVariable(&quot;callingProcessId&quot;,callingProcessId);
kcontext.setVariable(&quot;sessionCode&quot;, userToken.getSessionCode());
QEventMessage defaultPage = new QEventMessage(&quot;EVT_MSG&quot;, &quot;DEFAULTPAGE&quot;);
defaultPageSessionFacts = new SessionFacts(userToken, userToken, defaultPage);
kcontext.setVariable(&quot;defaultPageSessionFacts&quot;, defaultPageSessionFacts);
results = new HashMap&lt;String,Object>();
kcontext.setVariable(&quot;results&quot;,results);
inputSessionCode = userToken.getSessionCode();
kcontext.setVariable(&quot;inputSessionCode&quot;, inputSessionCode);
System.out.println(&quot;userLifeCycle: entered: pid &quot;+kcontext.getProcessInstance().getId()+&quot;:&quot;+((GennyToken)userToken).getUserCode()+&quot;:inputSessionCode = &quot;+inputSessionCode); 

/*
